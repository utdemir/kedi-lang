fn positive? n
  : number? n
  : bool? @return {
    return gt n 0;
}
 
fn fibonacci nth
    : positive? nth
    : positive? @return {
    let ret = 1;
    let prev = 0;

    let i = 1;
    while lte i nth {
        let temp = ret;
        ret = add ret prev;
        prev = temp;
        i = add i 1;
    }

    return ret;
}
